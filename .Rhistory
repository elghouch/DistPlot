run_app <- function() {
require(shiny)
ui <- fluidPage(
sliderInput(
inputId = "range",
label = "Interval (a,b):",
min = -5, max = 5, value = c(a = -1, b = 1), step = 0.5,
animate = animationOptions(interval = 800, loop = TRUE)
),
plotOutput("plot", height = "500px")
)
server <- function(input, output) {
output$plot <- renderPlot({
plot_dens(a = input$range[1], b = input$range[2])
})
}
runApp(shinyApp(ui, server))
}
run_app()
plot_dens <- function(a=-1, b=1) {
x <- seq(-5,5,length=200)
dx <- dnorm(x)
plot(x, dx, type="l", xlab="", ylab = "density")
i <- (x >= a) & (x <= b)
polygon(c(a,x[i],b), c(0,dx[i],0), col="lightgray")
text(a-0.2, 0.1, "a")
text(b+0.2, 0.1, "b")
title(main = "Normal density", sub = paste("P(a < X < b)  = ",
round(pnorm(b) - pnorm(a), 3)), xlab= NULL)
}
run_app <- function() {
require(shiny)
ui <- fluidPage(
sliderInput(
inputId = "range",
label = "Interval (a,b):",
min = -5, max = 5, value = c(a = -1, b = 1), step = 0.5
),
plotOutput("plot")
)
server <- function(input, output) {
output$plot <- renderPlot({
plot_dens()
})
}
runApp(shinyApp(ui, server))
}
run_app()
