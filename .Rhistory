plot(x, dx, type="l", xlab="", ylab = "density")
i <- (x >= a) & (x <= b)
polygon(c(a,x[i],b), c(0,dx[i],0), col="lightgray")
text(a-0.2, 0.1, "a")
text(b+0.2, 0.1, "b")
title(main = "Normal density", sub = paste("P(a < X < b)  =",
round(pnorm(b) - pnorm(a), 3)), xlab= NULL)
}
